  "type": "module",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch --preserveWatchOutput",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "ava --timeout=5m --serial",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/.bin/typeorm",
    "migration:generate": "yarn run typeorm migration:generate -d src/shared/config/datasource.config.ts",
    "migration:create": "yarn run typeorm migration:create -d src/shared/config/datasource.config.ts",
    "migration:run": "yarn run typeorm migration:run -d src/shared/config/datasource.config.ts",
    "migration:revert": "npm run typeorm -- migration:revert -d src/shared/config/datasource.config.ts"
  },
